{
    "name": "simple-react-boilerplate",
    "version": "0.0.1",
    "description": "An adapted copy of this react starter kit https://github.com/leonardomso/react-bolt created by Leonardo Maldonado <leonardomso11@gmail.com>. The main goal is to provide a simple boilerplate for small react-apps to be used in django based software maintained by the GFBio e.V.",
    "main": "index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --open",
        "build": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack",
        "lint": "eslint ./src/**/**.js",
        "lint:fix": "eslint ./src/**/**.js --fix",
        "test": "jest",
        "test:watch": "yarn run test --watch",
        "test:cover": "yarn run test --coverage",
        "cypress:open": "cypress open",
        "precommit": "yarn run lint && yarn test",
        "prepush": "yarn run lint"
    },
    "keywords": [],
    "author": "Marc Weber <mweber@gfbio.org>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.gwdg.de/gfbio/simple-react-boilerplate.git"
    },
    "bugs": {
        "url": "https://gitlab.gwdg.de/gfbio/simple-react-boilerplate/-/issues"
    },
    "homepage": "https://gitlab.gwdg.de/gfbio/simple-react-boilerplate#readme",
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/node": "^7.14.7",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@babel/plugin-transform-async-to-generator": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-react": "^7.14.5",
        "@babel/register": "^7.14.5",
        "@babel/runtime": "^7.14.6",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^3.0.0",
        "eslint": "^7.31.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.5.2",
        "img-loader": "^3.0.2",
        "jest": "^26.6.3",
        "jest-dom": "^4.0.0",
        "prettier": "^2.3.2",
        "react-testing-library": "^8.0.1",
        "url-loader": "^4.1.1",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.2",
        "webpack-jarvis": "^0.3.2",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "@material-ui/core": "^4.12.4",
        "@types/react-stickynode": "^4.0.0",
        "axios": "^0.21.4",
        "bootstrap": "^5.0.2",
        "cross-env": "^7.0.3",
        "formik": "^2.2.9",
        "nanoid": "^3.1.23",
        "prop-types": "^15.7.2",
        "query-string": "^7.1.1",
        "react": "^16.14.0",
        "react-bootstrap": "^1.6.1",
        "react-dom": "^16.14.0",
        "react-loadingg": "^1.7.2",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-stickynode": "^4.0.0",
        "redux": "^4.1.0",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "styled-components": "^5.3.0",
        "uuid": "^9.0.1",
        "validator": "^13.7.0"
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/config/tests/jest.config"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest"
        }
    }
}
